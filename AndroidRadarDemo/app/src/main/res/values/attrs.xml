<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RadarView">
        <attr name="circleColor" format="color|reference" />
        <attr name="polygonColor" format="color|reference" />
        <attr name="polygonCnt" format="integer|reference" />
        <attr name="polygonHierarchy" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="RadioGridView">
        <attr name="bg_item_selected" format="reference" />
        <attr name="bg_item_unselected" format="reference" />
        <attr name="tv_item_selected" format="color" />
        <attr name="tv_item_unselected" format="color" />
    </declare-styleable>

</resources>